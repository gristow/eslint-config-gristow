{
  "name": "eslint-config-gristow",
  "version": "3.0.0",
  "description": "ESLint 9 flat config for JavaScript and TypeScript projects",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./eslint.config.js": "./eslint.config.js"
  },
  "scripts": {
    "lint": "eslint . --ignore-pattern test/fixtures/",
    "lint:fix": "eslint . --fix --ignore-pattern test/fixtures/",
    "test": "node --test test/rules.test.js",
    "test:validate": "node test/validate-config.js"
  },
  "author": "Greg Ristow",
  "license": "ISC",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "typescript",
    "flat-config"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gristow/eslint-config-gristow"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@eslint/js": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.0.0",
    "typescript-eslint": "^8.0.0"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "typescript": "^5.0.0"
  }
}
